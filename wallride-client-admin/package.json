{
  "name": "wallride",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "gulp-systemjs-builder": "^0.15.0",
    "gulp-webserver": "^0.9.1",
    "jspm": "^0.17.0-beta.32"
  },
  "scripts": {
    "jspm": "jspm",
    "build": "gulp build",
    "server": "gulp server",
    "install": "jspm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "blueimp/jQuery-File-Upload": "github:blueimp/jQuery-File-Upload@9.8.1",
      "bootstrap": "github:twbs/bootstrap@3.3.1",
      "css": "github:systemjs/plugin-css@^0.1.32",
      "froala-editor": "npm:froala-editor@2.3.0",
      "ilikenwf/nestedSortable": "github:ilikenwf/nestedSortable@2.0",
      "jquery": "npm:jquery@2.2.4",
      "jquery-ui": "github:components/jqueryui@1.11.2",
      "moment/moment": "github:moment/moment@^2.15.1",
      "nylen/shiftcheckbox": "github:nylen/shiftcheckbox@0.8.0",
      "riot": "npm:riot@^3.0.5",
      "riot-route": "npm:riot-route@^3.0.2",
      "select2": "github:select2/select2@3.5.3",
      "xdan/datetimepicker": "github:xdan/datetimepicker@2.3.4"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "bcrypt-pbkdf": "npm:bcrypt-pbkdf@^1.0.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "cluster": "npm:jspm-nodelibs-cluster@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "dgram": "npm:jspm-nodelibs-dgram@^0.2.0",
      "dns": "npm:jspm-nodelibs-dns@^0.2.0",
      "ecc-jsbn": "npm:ecc-jsbn@~0.1.1",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "fsevents": "npm:fsevents@^1.0.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "jodid25519": "npm:jodid25519@^1.0.0",
      "jsbn": "npm:jsbn@0.1",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "punycode": "npm:jspm-nodelibs-punycode@^0.2.0",
      "querystring": "npm:jspm-nodelibs-querystring@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "tls": "npm:jspm-nodelibs-tls@^0.2.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "tweetnacl": "npm:tweetnacl@0.14",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "github:components/jqueryui@1.11.2": {
        "main": "jquery-ui",
        "dependencies": {
          "jquery": "*"
        }
      },
      "github:moment/moment@2.17.1": {
        "main": "moment",
        "format": "global",
        "meta": {
          "locale/*.js": {
            "deps": [
              "../moment"
            ],
            "format": "global"
          }
        }
      },
      "github:xdan/datetimepicker@2.3.4": {
        "dependencies": {
          "jquery": "*"
        },
        "meta": {
          "jquery.datetimepicker.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "github:blueimp/jQuery-File-Upload@9.8.1": {
        "main": "js/jquery.fileupload",
        "dependencies": {
          "jquery-ui": "*"
        },
        "map": {
          "jquery.ui.widget": "jquery-ui/ui/widget.js"
        },
        "meta": {
          "js/jquery.fileupload.js": {
            "deps": [
              "jquery-ui"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "github:ilikenwf/nestedSortable@2.0.0": {
        "main": "jquery.mjs.nestedSortable",
        "dependencies": {
          "jquery-ui": "*"
        },
        "map": {
          "jquery-ui/sortable": "jquery-ui/ui/sortable.js"
        }
      },
      "github:nylen/shiftcheckbox@0.8.0": {
        "main": "jquery.shiftcheckbox",
        "files": [
          "jquery.shiftcheckbox.js"
        ],
        "dependencies": {
          "jquery": "*"
        },
        "meta": {
          "jquery.shiftcheckbox.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "github:select2/select2@3.5.3": {
        "files": [
          "*.js",
          "*.template",
          "select2.png",
          "select2.css",
          "select2-spinner.gif"
        ],
        "registry": "jspm",
        "dependencies": {
          "jquery": "*",
          "css": "*"
        },
        "meta": {
          "select2.js": {
            "deps": [
              "jquery",
              "./select2.css!"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "github:twbs/bootstrap@3.3.1": {
        "registry": "jspm",
        "dependencies": {
          "jquery": "*",
          "css": "*"
        },
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery",
              "../css/bootstrap.css!"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:froala-editor@2.3.0": {
        "main": "js/froala_editor.min.js",
        "dependencies": {
          "jquery": "*"
        },
        "meta": {
          "js/froala_editor.min.js": {
            "deps": [
              "jquery"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.6.3": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.4": {
        "format": "amd"
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:readable-stream@1.1.14": {
        "map": {
          "stream": "stream-browserify/index"
        },
        "systemjs": {
          "main": "readable.js"
        },
        "dependencies": {
          "core-util-is": "~1.0.0",
          "inherits": "~2.0.1",
          "isarray": "0.0.1",
          "string_decoder": "~0.10.0",
          "stream-browserify": "^1.0.0"
        }
      }
    }
  }
}
